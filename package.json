{
  "name": "valiton-valiton-mongodbatlas-datasource",
  "version": "3.0.0",
  "description": "Display MongoDB Atlas metrics inside of Grafana",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "lint": "grafana-toolkit plugin:lint",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "lint": "prettier -c .",
    "start": "yarn watch"
  },
  "author": "Christoph Caprano",
  "license": "MIT",
  "devDependencies": {
    "@grafana/data": "latest",
    "@grafana/runtime": "latest",
    "@grafana/toolkit": "latest",
    "@grafana/ui": "latest",
    "@types/lodash": "latest"
  },
  "resolutions": {
    "rxjs": "6.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "humanize-string": "^2.1.0"
  }
}
